<% if (resultPreSale.length) { %>
<h2>本次新增入网楼栋：<%= resultPreSale.length %>个</h2>
<ul>
  <% resultPreSale.forEach(function(item){ %>
  <li>
    <p>
      <a href="<%= item.url %>">
        项目名称：<%= item.entryName %>
      </a>
    </p>
    <p>楼幢号：<%= item.buildingNumber %></p>
    <p>许可面积(㎡)：<%= item.permittedArea %></p>
    <p>发放日期：<%= item.time %></p>
  </li>
  <% }); %>
</ul>
<% } %>

<% if (resultList.length) { %>
<h2>本次新增摇号项目：<%= resultList.length %>个</h2>
<ul>
  <% resultList.forEach(function(item){ %>
  <li>
    <p>
      <a href="<%= item.url %>">
        项目名称：<%= item.entryName %>
      </a>
    </p>
    <p>区域：<span class="region"><%= item.region %></span></p>
    <p>总数：<%= item.total %></p>
    <p>登记状态：<%= item.registrationStatus %></p>

    <details>
      <summary>展开其他信息</summary>
      <p>企业名称：<%= item.enterpriseName %></p>
      <p>楼幢：
        <% item.building.forEach(function(li){ %>
        <a class="child-href" href='<%= li.url %>'><%= li.name %></a>
        <% }); %>
      </p>
      <p>刚需供应数：<%= Math.ceil(item.total * 0.3) %></p>
      <% if (['高新区', '经济区'].includes(item.region)) { %>
      <p>职住平衡供应数：Math.ceil(item.total * 0.5)</p>
      <% } %>
      <p>登记开始时间：<%= item.start %></p>
      <p>登记结束时间：<%= item.end %></p>
      <p>联系电话：
        <% item.telephone.forEach(function(phone){ %>
        <a class="child-href" href='tel:<%= phone %>'><%= phone %></a>
        <% }); %>
      </p>
      <p>售楼部地址：<%= item.salesDepartmentAddress %></p>
    </details>
  </li>
  <% }); %>
</ul>
<% } %>

<p>本次爬取时间：<%= currentTime %></p>

<style>
  a {
    color: #1677ff;
  }

  .child-href {
    text-decoration: none;
    color: #4a90e2;
  }
</style>